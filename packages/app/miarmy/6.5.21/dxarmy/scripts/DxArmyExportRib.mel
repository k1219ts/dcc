global proc DxArmyExportRib(int $mode, int $start, int $end, string $outdir) {
    McdInitMiarmy;
    python(
        "import dxArmy; dxArmy.ExportRib("
        + $mode + ","
        + $start + ","
        + $end + ","
        + "'" + $outdir + "'"
        + ")"
    );
}
